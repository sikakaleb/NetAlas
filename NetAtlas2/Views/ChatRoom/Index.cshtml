                                                            <h3>Top Profiles</h3>
                                                            <i class="la la-ellipsis-v"></i>
                                                        </div>
                                                        <div class="profiles-slider">

                                                            @if (@ViewBag.Search == "no corresponding response to the search")
                                                            {
                                                            <h4 class="text-danger text-center">@ViewBag.Search</h4>
                                                            }
                                                            else if (@ViewBag.Search == "yes")
                                                            {
                                                            foreach (var user in ViewBag.stranger)
                                                            {

                                                            Boolean isId = false;
                                                            Boolean estdedans = false;
                                                            Boolean estDejaInvite = false;
                                                            <div class="user-profy">
                                                                @if (user.ImageUrl.Length > 0)
                                                                {
                                                                    <img src="@Url.Content(user.ImageUrl)" alt="no image" style="height:64px" class="image-responsive img-circle" />
                                                                }
                                                                else
                                                                {
                                                                    <img src="@Url.Content("~/Profile Images/profil_vide.png")" alt="no image" style="height:64px" class="image-responsive img-circle" />

                                                                }
                                                                <h3> @user.UserName</h3>
                                                                <span>Bio</span>
                                                                @if (ViewBag.Id == user.Id)
                                                                {
                                                                    isId = true;
                                                                    using (Html.BeginForm("InviteFriend", "Friends"))
                                                                    {
                                                                        <ul>
                                                                            <li><a href="#" title="" class="envlp">Is you</a></li>
                                                                        </ul>
                                                                    }
                                                                }
                                                                @if (isId == false)
                                                                {

                                                                    foreach (var l in ((List<int>)ViewData["listfriend"]))
                                                                    {
                                                                        if (@l == user.Id)
                                                                        {
                                                                            estdedans = true;
                                                                            <ul>
                                                                                <li><a href="@Url.Action("UnAddfriend", "Friends", new { id = @user.Id })" title="" class="envlp ">Remove</a></li>
                                                                            </ul>
                                                                            break;
                                                                        }

                                                                    }
                                                                }
                                                                @if (estdedans == false && isId == false)
                                                                {

                                                                    foreach (var l in ((List<int>)ViewData["listinvitation"]))
                                                                    {
                                                                        if (@l == user.Id)
                                                                        {

                                                                            estDejaInvite = true;
                                                                            <ul>
                                                                                <li><a href="#" title="" class="hire">Already invite</a></li>
                                                                                <li><a  href="@Url.Action("Abort", "Friends", new { id = @user.Id })" title="" class="envlp">Abort</a></li>
                                                                            </ul>
                                                                            break;

                                                                        }
                                                                    }
                                                                }
                                                                @if (estdedans == false && isId == false && estDejaInvite == false)
                                                                {
                                                                    <ul>
                                                                        <li><a href="@Url.Action("InviteFriend", "Friends", new { id = @user.Id })" title="" class="followw">Add friend</a></li>
                                                                    </ul>
                                                                }

                                                                <a href="#" title="">View Profile</a>
                                                            </div>
                                                            }
                                                            }


                                                            <!--user-profy end-->
                                                            <!--user-profy end-->
                                                            <!--user-profy end-->
                                                            <!--user-profy end-->
                                                            <!--user-profy end-->
                                                            <!--user-profy end-->
                                                        </div>
                                                        <!--profiles-slider end-->
                                                    </div>
                                                }
                                                <!--top-profiles end-->
                                            }
                                        }
                                        <!---<div class="posty">
    </div>-->
                                        <!--posty end-->

