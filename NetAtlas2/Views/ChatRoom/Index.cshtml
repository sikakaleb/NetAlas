                                                                {
                                                                    <li><a href="@Url.Action("DeletePublication", "ChatRoom", new { id = publication.Id })" title="">Delete Post</a></li>
                                                                }
                                                                else
                                                                {
                                                                    <li><a href="#" title="">Delete Post</a></li>
                                                                }
                                                                <li><a href="#" title="">Close</a></li>
                                                            </ul>
                                                        </div>
                                                    </div>
