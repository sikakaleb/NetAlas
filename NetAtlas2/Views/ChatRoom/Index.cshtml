                                            int a = 0;
                                            foreach (var publication in Model)
                                            {
                                                a += 1;
                                                <div class="post-bar">
                                                    <div class="post_topbar">
                                                        <div class="usy-dt">
                                                            
                                                            @if (publication.User.ImageUrl.Length > 0)
